{
    "name": "angular-bootstrap",
    "description": "Angular UI Bootstrap support",
    "version": "0.7.0",
    "keywords": [
        "esp",
    ],
    "dependencies": {
        "angular": ">=1.2.0", 
        "bootstrap": ">=3.0.0", 
        "html5shiv": "*",
    },
    "export": [
        {
            from: "*.js",
            to: '${JSLIB}/angular-bootstrap',
        }
    ],
    "client-scripts": [ 
        "${JSLIB}/angular-bootstrap/*.js" 
    ]
}
